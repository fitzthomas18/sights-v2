{
  "name": "sights",
  "version": "2.0.0-unstable",
  "description": "A complete teleoperative robot control interface",
  "private": true,
  "type": "module",
  "scripts": {
    "sights:start": "vite client",
    "sights:build": "tsc --project client && vite build client",
    "sights:serve": "vite preview client",
    "sights:test": "vitest --config client/vite.config.ts",
    "test:ts": "tsc --project client --noEmit",
    "generate-client": "openapi --input http://127.0.0.1:8000/api/openapi.json --output ./client/src/api --client axios --name AppClient",
    "docs:start": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress preview docs"
  },
  "proxy": "http://127.0.0.1:8000",
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^24.3.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "highlight.js": "^11.8.0",
    "openapi-fetch": "^0.4.0",
    "prism-react-renderer": "^2.0.5",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-hotkeys-hook": "^4.4.0",
    "react-icons": "^4.10.1",
    "react-simple-code-editor": "^0.13.1",
    "tailwind-config": "^0.1.2",
    "ts-key-enum": "^2.0.12",
    "typescript": "^5.6.0",
    "use-editable": "^2.3.3",
    "usehooks-ts": "^2.9.1",
    "vite": "^7.1.5",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.7.0",
    "@babel/plugin-transform-private-property-in-object": "^7.25.0",
    "@eslint/config-array": "^0.19.0",
    "@eslint/object-schema": "^2.1.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.9",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.15.0",
    "glob": "^11.0.0",
    "jsdom": "^26.1.0",
    "openapi-typescript-codegen": "^0.29.0",
    "react-router-dom": "^6.14.0",
    "rimraf": "^6.0.0",
    "tailwindcss": "^4.1.13",
    "vitepress": "^2.0.0-alpha.12",
    "vitest": "^3.2.4",
    "vue": "^3.5.21"
  },
  "eslintConfig": {
    "env": {
      "vitest/globals": true
    },
    "plugins": [
      "@vitest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
