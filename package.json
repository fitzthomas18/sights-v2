{
  "name": "sights",
  "version": "2.0.0-unstable",
  "description": "A complete teleoperative robot control interface",
  "private": true,
  "type": "module",
  "scripts": {
    "sights:dev": "vite client",
    "sights:build": "tsc --project client && vite build client",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "generate-client": "openapi --input http://127.0.0.1:8000/api/openapi.json --output ./client/src/api --client axios --name AppClient",
    "ts:format": "prettier --write '**/*.{tsx,ts,js}'",
    "ts:check": "tsc --project client --noEmit"
  },
  "proxy": "http://127.0.0.1:8000",
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^24.3.1",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "axios": "^1.4.0",
    "chart.js": "^4.5.0",
    "form-data": "^4.0.4",
    "prettier": "^3.6.2",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.30.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-hotkeys-hook": "^4.4.0",
    "react-icons": "^4.10.1",
    "react-simple-code-editor": "^0.14.1",
    "ts-key-enum": "^2.0.12",
    "typescript": "^5.6.0",
    "usehooks-ts": "^2.9.1",
    "vite": "^7.1.5",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "openapi-typescript-codegen": "^0.29.0",
    "react-router-dom": "^6.14.0",
    "tailwindcss": "^4.1.13",
    "terser": "^5.44.0",
    "vitepress": "^2.0.0-alpha.12",
    "vue": "^3.5.21"
  },
  "eslintConfig": {
    "env": {
      "vitest/globals": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
